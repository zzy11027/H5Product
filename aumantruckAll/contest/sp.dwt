    function find(){

        var inp_1 = $('.inp_1').val();

        var inp_2 = $('.inp_2').val();
        if (inp_1=='' && inp_2=='') {
            alert("至少输入一项");
            return false;
        }

        $.ajax({

           type: 'POST',
           dataType:'json',
            url: "spike_contest.php?act=spacx",

            data:{cph:inp_1,qy:inp_2},

            success: function (res) {
                console.log(res.length)
                if(res.cph==inp_1){
                    $('.tan').show();
                    $('.containerss').show();
                    alert('车牌查询')
                    $('.contai').find('.con_1').text(res.cph);
                    $('.contai').find('.con_2').text(res.qycm);
                    $('.contai').find('.con_3').text(res.bgl);
                    $('.contai').find('.con_4').text(res.zlc);
                    $('.contai').find('.con_5').text(res.cx);
                    $('.contai').find('.con_6').text(res.ml);
                    $('.contai').find('.con_7').text();
                    $('.contai').find('.con_8').text(res.zz);
                    $('.shu').text(res.fs+'%');
                    $('.shu_1').text(res.cq+'%');
                    //点击分享出现二维码
                    var id_1 = res.id;
                    $('.img_3').attr('src','http://www.aumantruck.com/spike_contest.php?act=sparwm&id='+id_1);
                    return false;
                }

                if (res) {

                    $('.tan').show();
                    $('.containerss').show();

                    console.log(res)
                    console.log(res.length)

                    $(res).each(function(i){

                        $('.contai_0').show();

                        console.log(i)

                        var parentdiv=$('<div></div>');  

                        parentdiv.attr('class','contai_'+i);

                        parentdiv.attr('id','contai'); 

                        parentdiv.appendTo($('.bao'));

                        console.log($('.contai_'+i))

                        for(var q=1;q<9;q++){

                            var childdiv=$('<div></div>');

                            childdiv.attr('class','con_'+q);

                            childdiv.appendTo($('.contai_'+i));

                        }

                        $('.contai_'+i).find('.con_1').text(res[i].cph);
                        $('.contai_'+i).find('.con_2').text(res[i].qycm);
                        $('.contai_'+i).find('.con_3').text(res[i].bgl);
                        $('.contai_'+i).find('.con_4').text(res[i].zlc);
                        $('.contai_'+i).find('.con_5').text(res[i].cx);
                        $('.contai_'+i).find('.con_6').text(res[i].ml);
                        $('.contai_'+i).find('.con_7').text();
                        $('.contai_'+i).find('.con_8').text(res[i].zz);
                        $('.shu').text(res[i].fs+'%');
                        $('.shu_1').text(res[i].cq+'%');

                        //点击分享出现二维码
                        var tu = $("<img src=''/>");
                        
                        tu.attr('class','img_'+i);
                        tu.attr('id','img_3');
                        tu.appendTo(".imim");
                        var id_2 = res[i].id;
                        $('.img_'+i).attr('src','http://www.aumantruck.com/spike_contest.php?act=sparwm&id='+id_2);
                    })


                }else{
                    alert("未查到车手信息");
                    return false;
                }
                
            }
        });
       
    }
 

    function le(){

        if(m>0){
